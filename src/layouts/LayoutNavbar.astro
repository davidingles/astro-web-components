---
import Layout from './Layout.astro';
import Navbar from '../components/Navbar.astro'


const menuItems = {
	Productos: 'productos',
	Inicio: 'inicio',
	Nosotros: 'about',
}

const menu = Object.entries(menuItems)

const { title } = Astro.props

---
<Layout title={title}>

    <Navbar menuPrincipal={menu}></Navbar>

</Layout>