---
import Home from '../components/Home.astro'

const { menuPrincipal } = Astro.props
---
<nav class="flex">
    <Home></Home>
    <ul class="flex flex-row gap-4 justify-end mr-4">
        {
            menuPrincipal.map( item => (
                <li>
                    <a href={item[1]}>{item[0]}</a>
                </li>
            ))
        }
    </ul>
</nav>